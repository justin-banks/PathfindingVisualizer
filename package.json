{
	"name": "pathfinding-visualizer-v3",
	"version": "0.1.0",
	"description": "test various pathfinding algorithms",
	"author": "Justin Banks",
	"main": "public/electron.js",
	"build": {
		"appId": "pathfinding.visualizer"
	},
	"homepage": "./",
	"private": true,
	"dependencies": {
		"@material-ui/core": "^4.11.0",
		"@material-ui/icons": "^4.9.1",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.3.2",
		"@testing-library/user-event": "^7.1.2",
		"cross-env": "^7.0.2",
		"electron-is-dev": "^1.2.0",
		"node-sass": "^4.14.1",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-scripts": "3.4.1"
	},
	"scripts": {
		"start:web": "react-scripts start",
		"build:web": "cross-env PUBLIC_URL=./ react-scripts build",
		"build:desktop": "electron-builder",
		"start:desktop": "npm run build:web && npm run build:desktop",
		"start": "concurrently \"cross-env BROWSER=none npm run start:web\" \"wait-on http://localhost:3000 && electron .\"",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"concurrently": "^5.2.0",
		"css-loader": "^4.2.0",
		"electron": "^9.1.2",
		"electron-builder": "^22.8.0",
		"sass-loader": "^9.0.3",
		"style-loader": "^1.2.1",
		"wait-on": "^5.1.0",
		"webpack-cli": "^3.3.12"
	}
}
